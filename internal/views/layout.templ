package views

import "github.com/FredericoBento/HandGame/internal/views/components"
import "github.com/FredericoBento/HandGame/internal/models"


templ Page(title string, headerTitle string, navlinks []models.RouteButton, content templ.Component) {
  <!DOCTYPE HTML>
  @components.Head()
  <html>
    <body>
      @components.Header(headerTitle, navlinks)
      <div id="contents">      
        @content
      </div>

      @components.Footer()
    </body>
  </html>
}
